        <Grid Grid.Row="2" Visibility="{Binding SummaryVisibility}">
            <Grid.RowDefinitions>
                <RowDefinition Height="25"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <StackPanel Orientation="Horizontal" Margin="0,0,5,0" HorizontalAlignment="Right" VerticalAlignment="Center">
                    <TextBlock Text="共包含 " 
                               Style="{StaticResource CommonLeftTextBlock}"></TextBlock>
                    <TextBlock Text="{Binding WarningCount}" Foreground="Red" FontWeight="Bold"
                               Style="{StaticResource CommonLeftTextBlock}"></TextBlock>
                    <TextBlock Text=" 种违禁字" 
                               Style="{StaticResource CommonLeftTextBlock}"></TextBlock>
                </StackPanel>
            </Grid>
            <Grid Grid.Row="1" x:Name="UnCheckWordChildrenGrid" Background="Transparent" Margin="25,0,5,0"></Grid>
            <ListBox Grid.Row="1" x:Name="listBox" BorderThickness="0" Background="Transparent" ItemsSource="{Binding UncheckedWordLists}"
                             ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Auto" SelectedIndex="-1"
                             ItemContainerStyle="{StaticResource ListBoxItemContainerStyle}" VerticalAlignment="Top" HorizontalAlignment="Stretch"
                             ScrollViewer.CanContentScroll="True" ManipulationBoundaryFeedback="ListBox_OnManipulationBoundaryFeedback">
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="25"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Background="Transparent" Margin="10,0,0,5" Height="30" Tag="{Binding}"
                                  Cursor="Hand" MouseDown="UnCheckWordGrid_MouseDown">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                                    <ToggleButton x:Name="Expander" Margin="0,5,5,0" Cursor="Hand"
                                                  IsChecked="{Binding IsSelected}"
                                                  HorizontalAlignment="Left" VerticalAlignment="Center" 
                                                  Style="{StaticResource ExpandCollapseToggleStyle}"/>
                                    <TextBlock x:Name="TextBlockPART_Header" Text="{Binding Name}" 
                                               FontSize="16" FontFamily="Microsoft YaHei"></TextBlock>
                                    <StackPanel Orientation="Horizontal" Margin="5,0,5,0" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="(共有 " FontSize="10" Style="{StaticResource CommonLeftTextBlock}"></TextBlock>
                                        <TextBlock Text="{Binding WarningCount}" FontSize="10" Style="{StaticResource CommonLeftTextBlock}"></TextBlock>
                                        <TextBlock Text=" 处)" FontSize="10" Style="{StaticResource CommonLeftTextBlock}"></TextBlock>
                                    </StackPanel>
                                </StackPanel>
                            </Grid>
                            <ListBox x:Name="listBoxChildren" Grid.Row="1" Grid.Column="1" BorderThickness="0" Background="Transparent" ItemsSource="{Binding Children}"
                                     ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Auto" SelectedIndex="-1"
                                     ItemContainerStyle="{StaticResource ListBoxItemContainerStyle}" VerticalAlignment="Top" HorizontalAlignment="Stretch"
                                     ScrollViewer.CanContentScroll="False" ScrollViewer.PanningMode="None" Visibility="Collapsed">
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <Border BorderThickness="0" BorderBrush="LightGray" Margin="0,0,0,5"
                                                Width="{Binding ElementName=UnCheckWordChildrenGrid,Path=ActualWidth}">
                                            <Grid Tag="{Binding}" Cursor="Hand" Height="40" 
                                                  MouseUp="UnCheckWordChildrenGrid_MouseUp"
                                                  MouseDown="UnCheckWordChildrenGrid_MouseDown">
                                                <Grid Background="LightGray" Opacity="0.5"></Grid>
                                                <TextBlock x:Name="TextBlockPART_Header2" Text="{Binding Name}" 
                                                           TextWrapping="Wrap" TextTrimming="CharacterEllipsis" 
                                                           MaxWidth="{Binding ElementName=UnCheckWordChildrenGrid,Path=ActualWidth}"
                                                           FontSize="14" Foreground="#777777" FontFamily="Microsoft YaHei">
                                                </TextBlock>
                                            </Grid>
                                        </Border>
                                        <DataTemplate.Triggers>
                                            <DataTrigger Binding="{Binding IsSelected}" Value="true">
                                                <Setter Property="TextElement.Foreground" TargetName="TextBlockPART_Header2" Value="#14a0ed" />
                                            </DataTrigger>
                                        </DataTemplate.Triggers>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <WrapPanel Orientation="Vertical"/>
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>
                            </ListBox>
                        </Grid>
                        <DataTemplate.Triggers>
                            <DataTrigger Binding="{Binding IsSelected}" Value="true">
                                <Setter Property="TextElement.Foreground" TargetName="TextBlockPART_Header" Value="#14a0ed" />
                                <Setter Property="Visibility" TargetName="listBoxChildren" Value="Visible" />
                            </DataTrigger>
                        </DataTemplate.Triggers>
                    </DataTemplate>
                </ListBox.ItemTemplate>
                <ListBox.ItemsPanel>
                    <ItemsPanelTemplate>
                        <WrapPanel Orientation="Vertical"/>
                    </ItemsPanelTemplate>
                </ListBox.ItemsPanel>
            </ListBox>
        </Grid>
    </Grid>
</UserControl>
