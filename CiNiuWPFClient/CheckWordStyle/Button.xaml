    <Style x:Key="ButtonFocusVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Rectangle Margin="0" StrokeThickness="2" Stroke="#40c4ff"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
